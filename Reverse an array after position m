#include <bits/stdc++.h> 
void reverseArray(vector<int> &arr , int m)
{
	// Write your code here.
	int s,e,i;
	s=m+1; //start from position m+1 and ending position is arr.size()-1 Now swap these two positions in an array until s<=e
	e=arr.size()-1;
	while(s<=e)
	{
		swap(arr[s],arr[e]);
		s++;
		e--;
	}
}
